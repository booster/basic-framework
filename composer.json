{
    "name": "reep/basic",
    "description": "Basic Framework",
    "type": "project",
    "require": {
		"php":">=8.3",
        "nyholm/psr7": "^1.8",
        "nyholm/psr7-server": "^1.1",
        "laminas/laminas-httphandlerrunner": "^2.11",
        "psr/http-server-middleware": "^1.0",
		"php-di/php-di":">=7.0.9"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.1"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Basic\\": "src/"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/",
			"Unit\\": "tests/Unit",
			"Integration\\": "tests/Integration"
		}
	},
    "authors": [
        {
            "name": "Rene Petersen",
            "email": "reneestruppetersen@gmail.com"
        }
    ],
    "minimum-stability": "stable"
